<h1><%= @place["name"] %></h1>

<% if session[:user_id] %>
<p>
  <a href="/entries/new?place_id=<%= @place["id"] %>">New Posts</a>
</p>

<h2>My Entries</h2>
<% for entry in @entries %>
  <h3><%= entry["title"] %></h3>
  <h4><%= entry["occurred_on"] %></h4>
  <p><%= entry["uploaded_image"] %></p>
  <p><%= entry["description"] %></p>
<% end %>

<% end %>



<p>
  <a href="/places">Back to All Places</a>
</p>